<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_form" model="ir.ui.view">
        <field name="name">stock.picking.form</field>
        <field name="model">stock.picking</field>
        <field eval="12" name="priority"/>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="print_properties" string="Properties for Print" groups="product_properties.group_properties_print">
                    <field name="print_properties">
                        <form string="Properties for print">
                            <group>
                                <group>
                                    <field name="name"/>
                                    <field name="print"/>
                                </group>
                            </group>
                        </form>
                        <tree string="Properties for print">
                            <field name="name"/>
                            <field name="print"/>
                        </tree>
                    </field>
                </page>
            </xpath>
            <xpath expr="//notebook/page[@name='extra']/group/group[2]" position="inside">
                <field name="use_product_description"/>
            </xpath>
        </field>
    </record>
</odoo>