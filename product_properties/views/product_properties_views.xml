        <record id="product_properties_print_action" model="ir.actions.act_window">
            <field name="name">For print</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.properties.print</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="product_properties_print_search_view"/>
        </record>

        <menuitem id="menu_product_properties_action" name="Product Properties" action="product_properties_action"
            parent="stock.menu_stock_inventory_control" sequence="999" groups="product_properties.group_el_pro_user"/>

        <menuitem id="menu_product_properties_all_action" parent="stock.menu_product_in_config_stock" name="Product properties"
            sequence="60" groups="base.group_no_one"/>

        <menuitem id="menu_product_properties_type_action" parent="menu_product_properties_all_action" action="product_properties_type_action"
            sequence="1" groups="base.group_no_one"/>

        <menuitem id="menu_product_properties_package_action" parent="menu_product_properties_all_action" action="product_properties_package_action"
            sequence="1" groups="base.group_no_one"/>

        <menuitem id="menu_product_properties_dropdown_action" parent="menu_product_properties_all_action" action="product_properties_dropdown_action"
            sequence="1" groups="base.group_no_one"/>

        <menuitem id="menu_product_properties_print_action" parent="menu_product_properties_all_action" action="product_properties_print_action"
            sequence="2" groups="base.group_no_one"/>

        <menuitem id="menu_product_properties_category_action" parent="menu_product_properties_all_action" action="product_properties_category_action"
            sequence="3" groups="base.group_no_one"/>

        <record id="product_category_form_view_inherit_properties" model="ir.ui.view">
            <field name="name">product.category.properties.form</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view" />
            <field name="arch" type="xml">
                <group name="first" position="after">
                    <group string="Product properties">
                        <field name="product_properties_ids" context="{'default_categ_id': active_id}" nolabel="1">
                            <form string="Product properties">
                                <sheet>
                                    <field name="image_medium" widget="image" class="oe_avatar" attrs="{'invisible':[('type_fields', '!=', 'package')]}"/>
                                    <div class="oe_title">
                                        <label class="oe_edit_only" for="name" string="Properties Name"/>
                                        <h1>
                                            <field name="name"/>
                                        </h1>
                                    </div>
                                    <group>
                                        <group>
                                            <field name="type_fields"/>
                                        </group>
                                        <group name="properties">
                                            <field name="type_float" attrs="{'invisible':[('type_fields', '!=', 'float')]}"/>
                                            <field name="type_char" attrs="{'invisible':[('type_fields', '!=', 'char')]}"/>
                                            <field name="type_int" attrs="{'invisible':[('type_fields', '!=', 'int')]}"/>
                                            <field name="type_boolean" attrs="{'invisible':[('type_fields', '!=', 'boolean')]}"/>
                                            <field name="type_url" widget="url" attrs="{'invisible':[('type_fields', '!=', 'url')]}"/>
                                            <field name="type_uom_id" attrs="{'invisible':[('type_fields', 'in', ('char', 'package', 'msl', 'type_field', 'url', 'dropdown_id'))]}" domain="[('name_id', '=', active_id)]" context="{'default_name_id': active_id}"/>
                                            <field name="type_dropdown_id" attrs="{'invisible':[('type_fields', '!=', 'dropdown_id')]}" domain="[('name_id', '=', name)]" context="{'default_name_id': name}"/>
                                        </group>
                                        <group string="Range" attrs="{'invisible':[('type_fields', '!=', 'range')]}">
                                            <field name="type_int" attrs="{'invisible':[('type_fields', 'not in', ('int', 'range'))]}"/>
                                            <field name="type_int_second" attrs="{'invisible':[('type_fields', 'not in', ('int', 'range'))]}"/>
                                        </group>
                                        <group string="Dimensions" attrs="{'invisible':[('type_fields', '!=', 'package')]}">
                                            <field name="type_package_id" attrs="{'invisible':[('type_fields', '!=', 'package')]}" domain="[('name_id', '=', active_id)]" context="{'default_name_id': active_id}"/>
                                            <field name="dimensions_x"/>
                                            <field name="dimensions_y"/>
                                            <field name="dimensions_z"/>
                                        </group>
                                    </group>
                                </sheet>
                            </form>
                            <tree string="Product properties">
                                <field name="sequence" widget="handle"/>
                                <field name="name"/>
                                <field name="type_display"/>
                                <!-- <field name="type_range"/> -->
                                <field name="type_display_attrs"/>
                                <field name="type_fields" invisible="1"/>
                                <field name="image_small" widget="image" attrs="{'invisible':[('type_fields', '!=', 'package')]}"/>
                            </tree>
                        </field>
                    </group>
                </group>
            </field>
        </record>

