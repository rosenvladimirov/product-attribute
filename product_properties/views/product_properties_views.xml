<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_properties_package_tree_view" model="ir.ui.view">
            <field name="name">product.properties.package.tree.view</field>
            <field name="model">product.properties.package</field>
            <field name="arch" type="xml">
                <tree string="Package-Corpus">
                    <field name="name"/>
                    <field name="dimensions_x"/>
                    <field name="dimensions_y"/>
                    <field name="dimensions_z"/>
                    <field name="image_small" widget="image"/>
                </tree>
            </field>
        </record>
        <record id="product_properties_package_form_view" model="ir.ui.view">
            <field name="name">product.properties.package.form.view</field>
            <field name="model">product.properties.package</field>
            <field name="arch" type="xml">
                <form string="Package-Corpus">
                    <sheet>
                        <field name="image_medium" widget="image" class="oe_avatar"/>
                        <div class="oe_title">
                            <label class="oe_edit_only" for="name" string="Package Name"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                        <group>
                            <group string="Dimensions">
                                <field name="dimensions_x"/>
                                <field name="dimensions_y"/>
                                <field name="dimensions_z"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="product_properties_package_search_view" model="ir.ui.view">
            <field name="name">product.properties.package.search.view</field>
            <field name="model">product.properties.package</field>
            <field name="arch" type="xml">
                <search string="Product properties package">
                    <field string="Name" name="name" filter_domain="[('name', '=', self)]"/>
                    <group expand="0" string="Group By">
                        <filter string="Name" name="groupby_name" domain="[]" context="{'group_by': 'name'}"/>
                    </group>
                </search>
            </field>
        </record>
        <record id="product_properties_package_action" model="ir.actions.act_window">
            <field name="name">Product properties packages</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.properties.package</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="product_properties_package_search_view"/>
        </record>

        <record id="product_properties_dropdown_tree_view" model="ir.ui.view">
            <field name="name">product.properties.dropdown.tree.view</field>
            <field name="model">product.properties.dropdown</field>
            <field name="arch" type="xml">
                <tree string="Product properties Dropdown">
                    <field name="code"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="product_properties_dropdown_view" model="ir.ui.view">
            <field name="name">product.properties.dropdown.form.view</field>
            <field name="model">product.properties.dropdown</field>
            <field name="arch" type="xml">
                <form string="Product properties Dropdown">
                    <sheet>
                        <group>
                            <field name="code"/>
                            <field name="name"/>
                            <field name="name_id" domain="[('type_fields', '=', 'dropdown_id')]"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="product_properties_dropdown_search_view" model="ir.ui.view">
            <field name="name">product.properties.dropdown.search.view</field>
            <field name="model">product.properties.dropdown</field>
            <field name="arch" type="xml">
                <search string="Product properties Dropdown">
                    <field string="Name" name="name" filter_domain="[('name', '=', self)]"/>
                    <group expand="0" string="Group By">
                        <filter string="Type" name="groupby_name_id" domain="[]" context="{'group_by': 'name_id'}"/>
                    </group>
                </search>
            </field>
        </record>
        <record id="product_properties_dropdown_action" model="ir.actions.act_window">
            <field name="name">Product properties dropdown</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.properties.dropdown</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="product_properties_dropdown_search_view"/>
        </record>

        <record id="product_properties_search_view" model="ir.ui.view">
            <field name="name">product.properties.search.view</field>
            <field name="model">product.properties</field>
            <field name="arch" type="xml">
                <search string="Product properties">
                    <field string="Product" name="product_id" filter_domain="[('product_id', '=', self)]"/>
                    <group expand="0" string="Group By">
                        <filter string="Product" name="groupby_product" domain="[]" context="{'group_by': 'product_id'}"/>
                    </group>
                </search>
            </field>
        </record>
        <record id="product_properties_tree_view" model="ir.ui.view">
            <field name="name">product.properties.view</field>
            <field name="model">product.properties</field>
            <field name="arch" type="xml">
                <tree string="Product properties">
                    <field name="sequence" widget="handle"/>
                    <field name="product_tmpl_id"/>
                    <field name="product_id"/>
                    <field name="categ_id"/>
                    <field name="name"/>
                    <field name="type_display"/>
                    <field name="type_display_attrs" attrs="{'invisible': [('type_fields', '!=', 'package')]}"/>
                    <field name="type_fields" invisible="1"/>
                    <field name="image_small" widget="image" attrs="{'invisible': [('type_fields', '!=', 'package')]}"/>
                </tree>
            </field>
        </record>
        <record id="product_properties_form_view" model="ir.ui.view">
            <field name="name">product.properties.form.view</field>
            <field name="model">product.properties</field>
            <field name="arch" type="xml">
                <form string="Product properties">
                    <sheet>
                        <field name="image_medium" widget="image" class="oe_avatar" attrs="{'invisible':[('type_fields', '!=', 'package')]}"/>
                        <div class="oe_title">
                            <label class="oe_edit_only" for="name" string="Properties Name"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                            <h2>
                                <field name="product_id" readonly="1"/>
                            </h2>
                        </div>
                        <group>
                            <group>
                                <field name="type_fields"/>
                            </group>
                            <group name="properties">
                                <field name="type_float" attrs="{'invisible':[('type_fields', '!=', 'float')]}"/>
                                <field name="type_char" attrs="{'invisible':[('type_fields', '!=', 'char')]}"/>
                                <field name="type_int" attrs="{'invisible':[('type_fields', '!=', 'int')]}"/>
                                <field name="type_boolean" attrs="{'invisible':[('type_fields', '!=', 'boolean')]}"/>
                                <field name="type_url" widget="url" attrs="{'invisible':[('type_fields', '!=', 'url')]}"/>
                                <field name="type_uom_id" attrs="{'invisible':[('type_fields', 'in', ('char', 'package', 'field', 'lot', 'url', 'dropdown_id', 'date'))]}" domain="[('name_id', '=', active_id)]" context="{'default_name_id': active_id}"/>
                                <field name="type_dropdown_id" attrs="{'invisible':[('type_fields', '!=', 'dropdown_id')]}" domain="[('name_id', '=', name)]" context="{'default_name_id': name}"/>
                                <field name="type_field" attrs="{'invisible':[('type_fields', '!=', 'field')]}"/>
                            </group>
                            <group name="fields" string="Fields sources" attrs="{'invisible':[('type_fields', '!=', 'field')]}">
                                <field name="type_field_model_id" readonly="0" options="{'no_create_edit': True}" attrs="{'invisible':[('type_fields', '!=', 'field')]}"/>
                                <field name="type_field_target" options="{'no_create_edit': True}" attrs="{'invisible':[('type_fields', '!=', 'field')]}"/>
                            </group>
                            <group string="Range" attrs="{'invisible':[('type_fields', '!=', 'range')]}">
                                <field name="type_int" attrs="{'invisible':[('type_fields', 'not in', ('int', 'range'))]}"/>
                                <field name="type_int_second" attrs="{'invisible':[('type_fields', 'not in', ('int', 'range'))]}"/>
                            </group>
                            <group string="Dimensions" attrs="{'invisible':[('type_fields', '!=', 'package')]}">
                                <field name="type_package_id" attrs="{'invisible':[('type_fields', '!=', 'package')]}" domain="[('name_id', '=', active_id)]" context="{'default_name_id': active_id}"/>
                                <field name="dimensions_x"/>
                                <field name="dimensions_y"/>
                                <field name="dimensions_z"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_product_properties_view">
            <field name="name">Product properties</field>
            <field name="res_model">product.properties</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('product_properties_tree_view')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('product_properties_form_view')})]"/>
        </record>

        <record id="product_properties_print_form_view" model="ir.ui.view">
            <field name="name">product.properties.print.form.view</field>
            <field name="model">product.properties.print</field>
            <field name="arch" type="xml">
                <form string="Properties for print">
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="print"/>
                            <field name="system_properties"/>
                        </group>
                    </group>
                </form>
            </field>
        </record>
        <record id="product_properties_print_tree_view" model="ir.ui.view">
            <field name="name">product.properties.print.tree.view</field>
            <field name="model">product.properties.print</field>
            <field name="arch" type="xml">
                <tree string="Properties for print">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="print"/>
                    <field name="system_properties"/>
                </tree>
            </field>
        </record>
        <record id="product_properties_print_search_view" model="ir.ui.view">
            <field name="name">product.properties.print.search.view</field>
            <field name="model">product.properties.print</field>
            <field name="arch" type="xml">
                <search string="Product properties for print">
                    <field string="Properties type" name="name" filter_domain="[('name', '=', self)]"/>
                    <group expand="0" string="Group By">
                        <filter string="Partner properties" name="groupby_partner_id" domain="[]" context="{'group_by': 'partner_id'}"/>
                        <filter string="Sale order properties" name="groupby_order_id" domain="[]" context="{'group_by': 'order_id'}"/>
                        <filter string="Invoice properties" name="groupby_invoice_id" domain="[]" context="{'group_by': 'invoice_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="product_properties_type_search_view" model="ir.ui.view">
            <field name="name">product.properties.type.search.view</field>
            <field name="model">product.properties.type</field>
            <field name="arch" type="xml">
                <search string="Product properties type">
                    <field string="Properties type" name="name" filter_domain="[('name', '=', self)]"/>
                    <group expand="0" string="Group By">
                        <filter string="Type properties" name="groupby_type" domain="[]" context="{'group_by': 'type_fields'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="product_properties_type_tree_view" model="ir.ui.view">
            <field name="name">product.properties.type.view</field>
            <field name="model">product.properties.type</field>
            <field name="arch" type="xml">
                <tree string="Product properties type">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="type_fields"/>
                </tree>
            </field>
        </record>
        <record id="product_properties_type_form_view" model="ir.ui.view">
            <field name="name">product.properties.type.form.view</field>
            <field name="model">product.properties.type</field>
            <field name="arch" type="xml">
                <form string="Product properties type">
                    <sheet>
                        <div class="oe_title">
                            <label class="oe_edit_only" for="name" string="Properties Name"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="type_fields"/>
                            </group>
                            <group name="properties">
                                <field name="type_float" attrs="{'invisible':[('type_fields', '!=', 'float')]}"/>
                                <field name="type_char" attrs="{'invisible':[('type_fields', '!=', 'char')]}"/>
                                <field name="type_int" attrs="{'invisible':[('type_fields', '!=', 'int')]}"/>
                                <field name="type_boolean" attrs="{'invisible':[('type_fields', '!=', 'boolean')]}"/>
                                <field name="type_url" widget="url" attrs="{'invisible':[('type_fields', '!=', 'url')]}"/>
                                <!-- <field name="type_uom_id" attrs="{'invisible':[('type_fields', 'in', ('char', 'package', 'field', 'lot', 'url', 'dropdown_id'))]}"/> -->
                                <!-- <field name="type_dropdown_id" attrs="{'invisible':[('type_fields', '!=', 'dropdown_id')]}" domain="[('name_id', '=', active_id)]" context="{'default_name_id': active_id}"/> -->
                                <field name="type_field" attrs="{'invisible':[('type_fields', '!=', 'field')]}"/>
                            </group>
                            <group name="fields" string="Fields sources" attrs="{'invisible':[('type_fields', '!=', 'field')]}">
                                <field name="type_field_model_id" readonly="0" options="{'no_create_edit': True}" attrs="{'invisible':[('type_fields', '!=', 'field')]}"/>
                                <field name="type_field_target" options="{'no_create_edit': True}" attrs="{'invisible':[('type_fields', '!=', 'field')]}"/>
                            </group>
                            <group string="Range" attrs="{'invisible':[('type_fields', '!=', 'range')]}">
                                <field name="type_int" attrs="{'invisible':[('type_fields', 'not in', ('int', 'range'))]}"/>
                                <field name="type_int_second" attrs="{'invisible':[('type_fields', 'not in', ('int', 'range'))]}"/>
                            </group>
                            <separator name="listings" attrs="{'invisible':[('type_fields', 'in', ('char', 'package', 'field', 'lot', 'url', 'dropdown_id', 'date'))]}" colspan="4"/>
                            <field name="type_uom_id" attrs="{'invisible':[('type_fields', 'in', ('char', 'package', 'field', 'lot', 'url', 'dropdown_id', 'date'))]}" domain="[('name_id', '=', active_id)]" context="{'default_name_id': active_id}">
                                <tree>
                                    <field name="sequence" widget="handle"/>
                                    <field name="name"/>
                                </tree>
                            </field>
                            <separator name="listings" attrs="{'invisible':[('type_fields', '!=', 'package')]}" colspan="4"/>
                            <field name="type_package_id" nolabel="1" colspan="4" attrs="{'invisible':[('type_fields', '!=', 'package')]}" domain="[('name_id', '=', active_id)]" context="{'default_name_id': active_id}">
                                <tree>
                                    <field name="dimensions_x"/>
                                    <field name="dimensions_y"/>
                                    <field name="dimensions_z"/>
                                </tree>
                            </field>
                            <separator name="dropdowns" attrs="{'invisible':[('type_fields', '!=', 'dropdown_id')]}" colspan="4"/>
                            <field name="type_dropdown_id" widget="many2many_list" nolabel="1" colspan="4" attrs="{'invisible':[('type_fields', '!=', 'dropdown_id')]}" domain="[('name_id', '=', active_id)]" context="{'default_name_id': active_id}">
                                <tree>
                                    <field name="sequence" widget="handle"/>
                                    <field name="code"/>
                                    <field name="name"/>
                                    <field name="name_id" invisible="1"/>
                                </tree>
                            </field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="product_properties_category_form_view" model="ir.ui.view">
            <field name="name">product.properties.category.form.view</field>
            <field name="model">product.properties.category</field>
            <field name="arch" type="xml">
                <form string="Properties for print">
                    <sheet>
                        <group>
                            <group colspan="4">
                                <field name="name"/>
                            </group>
                            <!-- <label for="lines_ids" colspan="4"/> -->
                            <field name="lines_ids" nolabel="1" colspan="4" context="{'system_properties': True}"/>
                            <field name="print_ids" nolabel="1" colspan="4"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="product_properties_category_lines_search_view" model="ir.ui.view">
            <field name="name">product.properties.category.lines.search.view</field>
            <field name="model">product.properties.category.lines</field>
            <field name="arch" type="xml">
                <search string="Product properties">
                    <field string="Internal category" name="product_categ_id" filter_domain="[('product_categ_id', '=', self)]"/>
                    <group expand="0" string="Group By">
                        <filter string="Internal category" name="groupby_product_categ_id" domain="[]" context="{'group_by': 'product_categ_id'}"/>
                    </group>
                </search>
            </field>
        </record>
        <record id="product_properties_category_lines_tree_view" model="ir.ui.view">
