<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Decimal precision for volume and dimension -->
    <record id="decimal_stock_volume" model="decimal.precision">
        <field name="name">Stock Volume</field>
        <field name="digits">3</field>
    </record>
    <record id="decimal_stock_volume" model="decimal.precision">
        <field name="name">Stock Volume Gallon</field>
        <field name="digits">2</field>
    </record>
    <record id="decimal_stock_dimension" model="decimal.precision">
        <field name="name">Stock Dimension</field>
        <field name="digits">1</field>
    </record>
    <record id="decimal_stock_dimension" model="decimal.precision">
        <field name="name">Stock Dimension Inch</field>
        <field name="digits">2</field>
    </record>
    <record id="decimal_stock_weight_g" model="decimal.precision">
        <field name="name">Stock Weight Gram</field>
        <field name="digits">0</field>
    </record>
    <record id="decimal_stock_weight_lb" model="decimal.precision">
        <field name="name">Stock Weight Pound</field>
        <field name="digits">0</field>
    </record>
    <record id="decimal_stock_weight_g" model="decimal.precision">
        <field name="name">Stock Weight Ounce</field>
        <field name="digits">2</field>
    </record>
</odoo>
